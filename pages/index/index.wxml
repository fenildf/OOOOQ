<!--index.wxml-->
<view class="container">
  <view class="userInfo">
    <image class="userAvatsr" src="https://m.teacher.jiaofuyun.com/studentPK/images/weapp/avatar.jpg"></image>
    <view class="name">倒车入库</view>
    <view class="schoolClass">安徽省合肥市168中学 九年级二班</view>
  </view>
  <view class="gameInfo">
    <view class="gameData">
      <view class="gameNum fl">
        <view>{{gameInfo.times}}</view>
        <view>作战数</view>
      </view>
      <view class="winNum fl">
        <view>{{gameInfo.win_times}}</view>
        <view>战胜数</view>
      </view>
      <view class="winProbablility fl">
        <view>{{gameInfo.winP}}</view>
        <view>胜率</view>
      </view>
    </view>
    <button open-type="getUserInfo" class="playGame" bindgetuserinfo="onGotUserInfo">
      随机匹配
    </button>
  </view>
</view>
